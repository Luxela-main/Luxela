{
  "name": "luxela",
  "version": "1.0.0",
  "packageManager": "pnpm@10.29.3",
  "scripts": {
    "dev": "cross-env NODE_ENV=development pnpm run dev:next",
    "dev:local": "concurrently -n NEXT,SERVER -c cyan,green \"pnpm run dev:server\" \"pnpm run dev:next\"",
    "dev:server": "echo 'Express server temporarily disabled - using Next.js API routes'",
    "dev:next": "next dev -p 3000",
    "dev:check-db": "ts-node-dev --respawn server/check-db.ts",
    "dev:express-server": "ts-node-dev --respawn --transpile-only server/index.ts",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "tsx server/db/migrate.ts",
    "build": "cross-env NODE_ENV=production pnpm run build:vercel",
    "build:local": "pnpm run db:generate && pnpm run build:server && pnpm run build:client",
    "build:server": "tsc --project tsconfig.server.json",
    "build:client": "next build",
    "build:vercel": "next build",
    "start": "next start",
    "start:local": "pnpm run dev:local",
    "start:server": "node dist/server/index.js",
    "lint": "next lint",
    "swagger:server": "ts-node server/openapi/server.ts",
    "type-check": "tsc --noEmit"
  },
  "engines": {
    "node": ">=24 || >=25",
    "pnpm": ">=10.20",
    "npm": "11.6.2"
  },
